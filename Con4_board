def getNewBoard():
    board = []
    for x in range(7):
        board.append([' '] * 6)
    return board
    
mainboard = getNewBoard()

def drawBoard(board):
    print('+--+--+--+--+--+--+--+')
    for y in range(6): # height of the matrix
       
        for x in range(1,7): # width of the matrix
            print('| {}' .format(board[x][y]), end="")
        print('|  |')
        print('+--+--+--+--+--+--+--+')

drawBoard(mainboard)
